		<anim name="ntl_rifle_idle_husk1_var1_l" weight="(1-akimbo_pistol) * husk1 * (cbt+hos) * vr" state_length_contrib="false"/>
		<anim name="cbt_rifle_turn_r_45_vr" weight="rifle * husk1 * (cbt+hos) * vr * angle45"/>
		<anim name="cbt_rifle_turn_r_90_vr" weight="rifle * husk1 * (cbt+hos) * vr * angle90"/>
		<anim name="cbt_rifle_turn_r_135_vr" weight="rifle * husk1 * (cbt+hos) * vr * angle135"/>
		<anim name="cbt_pistol_turn_r_45_vr" weight="pistol * husk1 * (cbt+hos) * vr * angle45"/>
		<anim name="cbt_pistol_turn_r_90_vr" weight="pistol * husk1 * (cbt+hos) * vr * angle90"/>
		<anim name="cbt_pistol_turn_r_135_vr" weight="pistol * husk1 * (cbt+hos) * vr * angle135"/>
		<anim name="cbt_bullpup_turn_r_45_vr" weight="bullpup * husk1 * (cbt+hos) * vr * angle45"/>
		<!-- <anim name="cbt_bullpup_turn_r_90_vr" weight="bullpup * husk1 * (cbt+hos) * vr"/> -->
		<!-- <anim name="cbt_bullpup_turn_r_135_vr" weight="bullpup * husk1 * (cbt+hos) * vr"/> -->
		<!--<anim name="cbt_bow_turn_r_45_vr" weight="bow * husk1 * (cbt+hos) * vr * angle45"/>-->
		<!--<anim name="cbt_bow_turn_r_90_vr" weight="bow * husk1 * (cbt+hos) * vr * angle90"/>-->
		<!--<anim name="cbt_bow_turn_r_135_vr" weight="bow * husk1 * (cbt+hos) * vr * angle135"/>
		<!-- End vr -->

