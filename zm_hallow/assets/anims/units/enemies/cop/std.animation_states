	<state name="std/crouch/move/run/fwd" type="mixloop" segment="base" speed="1">
		<anim name="hos_legs_crh_run_fwd" weight="(hos + ntl) * (1 - wnd) * (1 - sup)"/>
		<anim name="cbt_legs_crh_run_fwd" weight="cbt * (1 - sup) * (1-shield)"/>
		<anim name="hos_legs_sup_run_fwd" weight="hos * sup  * (1 - wnd)"/>
		<anim name="cbt_legs_sup_run_fwd" weight="cbt * sup * (1-shield)"/>

		<anim name="hos_rifle_crh_run_fwd" weight="(hos + ntl) * (1 - wnd) * (1 - sup) * rifle * (1-vr)"/>
		<anim name="cbt_rifle_crh_run_fwd" weight="cbt * (1 - sup) * rifle * (1-vr)"/>
		<anim name="hos_pistol_crh_run_fwd" weight="(hos + ntl - wnd) * (1 - sup) * pistol * (1-vr)"/>
		<anim name="cbt_pistol_crh_run_fwd" weight="cbt * (1 - sup) * pistol * (1-shield) * (1-vr)"/>
		<anim name="hos_akimbo_pistol_crh_run_fwd" weight="(hos + ntl) * akimbo_pistol"/>
		<anim name="cbt_akimbo_pistol_crh_run_fwd" weight="cbt * akimbo_pistol"/>
		
		<anim name="hos_bow_crh_run_fwd" weight="(hos + ntl) * bow * (1-vr)"/>
		<anim name="cbt_bow_crh_run_fwd" weight="cbt * bow * (1-vr)"/>

		<anim name="cbt_bullpup_crh_run_fwd" weight="(cbt + hos + ntl) * bullpup * (1-vr)"/>

		<anim name="hos_rifle_sup_run_fwd" weight="(hos + ntl) * sup * (1 - wnd) * rifle * (1-vr)"/>
		<anim name="cbt_rifle_sup_run_fwd" weight="cbt * sup * rifle * (1-vr)"/>
		<anim name="hos_pistol_sup_run_fwd" weight="(hos + ntl) * sup * (1 - wnd) * pistol * (1-vr)"/>
		<anim name="cbt_pistol_sup_run_fwd" weight="cbt * sup * pistol * (1-shield) * (1-vr)"/>

		<anim name="cbt_shield_crh_run_fwd" weight="shield"/>

		<anim name="rifle_wounded_run_fwd_1" weight="wnd - cbt"/>

		<!-- VR --> 
		<anim name="ntl_rifle_idle_husk1_var1_l" weight="(1-akimbo_pistol) * husk1 * (cbt+hos) * vr" state_length_contrib="false"/>
		<anim name="cbt_rifle_crh_run_fwd_vr" weight="rifle * husk1 * (cbt+hos) * vr"/>
		<anim name="cbt_pistol_crh_run_fwd_vr" weight="pistol * husk1 * (cbt+hos) * vr"/>
		<anim name="cbt_bullpup_crh_run_fwd_vr" weight="bullpup * husk1 * (cbt+hos) * vr"/>
		<!--<anim name="cbt_bow_crh_run_fwd_vr" weight="bow * husk1 * (cbt+hos) * vr"/>-->
		<!-- End VR -->

		<to name="stand" redirect="std/stand/move/run/fwd"/>
		<to name="stop" redirect="std/crouch/move/run/stop/fwd"/>
		<keys>
			<key at="enter" set_extension="anim_data" extension_key="move_fwd" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="move_fwd" extension_value="false"/>
			<key at="enter" set_extension="anim_data" extension_key="move_side" extension_value="fwd"/>
			<key at="exit" set_extension="anim_data" extension_key="move_side" extension_value="false"/>
		</keys>
	</state>

