	<state name="std/crouch" type="template_state" segment="base">
		<from_group name="std/stand" blend="19"/>
		<to name="stand" redirect="std/stand/still/idle/look"/>
		<to name="reload" redirect="upper_body/crouch_reload"/>
		<to name="reload_looped_start" redirect="upper_body/crouch_reload_loop_enter"/>
		<to name="reload_looped" redirect="upper_body/crouch_reload_loop"/>
		<to name="reload_looped_exit" redirect="upper_body/crouch_reload_loop_exit"/>

		<to name="recoil_auto" redirect="upper_body/crh/recoil/auto"/>
		<to name="recoil_auto_enter" redirect="upper_body/crh/recoil/auto_enter"/>
		<to name="recoil_auto_exit" redirect="upper_body/crh/recoil/auto_exit"/>
		<to name="recoil_single" redirect="upper_body/crh/recoil/single"/>
