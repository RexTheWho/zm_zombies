<!-- Revive Interaction -->
<!-- Have to be their own states because we use a loop which we can't hold over a param from the enter state -->
	<state name="upper_body/stand_revive_enter" type="mix" segment="upper_body">
		<anim name="cbt_std_revive_enter_rifle" weight="is_rifle + is_lmg + is_smg + is_shotgun_pump + is_shotgun_mag"/>
		<anim name="cbt_std_revive_enter_pistol" weight="is_pistol + is_revolver"/>
		<anim name="cbt_std_revive_enter_bullpup" weight="is_bullpup + uzi"/>

		<block name="upper_body/std/recoil/auto_exit"/>
		<default blend="5" />
		<keys timebase="relative">
			<key at="enter" set_extension="anim_data" extension_key="interact" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="interact" extension_value="false"/>
			<key at="enter" callback="anim_clbk_wanted_item" class_name="movement" param1="needle" param2="hand_l" param3="true"/>
			<key at="exit" callback="anim_clbk_flush_wanted_items" class_name="movement"/>
			<key at="enter" callback="anim_clbk_hide_akimbo_weapon" class_name="movement"/>
			<key at="exit" callback="anim_clbk_show_akimbo_weapon" class_name="movement"/>
		</keys>
		<exit name="upper_body/stand_revive_loop"/>
	</state>

	<state name="upper_body/stand_revive_loop" type="mixloop" segment="upper_body">
		<anim name="cbt_std_revive_loop_rifle" weight="is_rifle + is_lmg + is_smg + is_shotgun_pump + is_shotgun_mag"/>
		<anim name="cbt_std_revive_loop_pistol" weight="is_pistol + is_revolver"/>
		<anim name="cbt_std_revive_loop_bullpup" weight="is_bullpup + uzi"/>

		<block name="upper_body/std/recoil/auto_exit"/>
		<to name="exit" redirect="upper_body/stand_revive_exit"/>
		<default blend="5" />
		<keys>
			<key at="enter" set_extension="anim_data" extension_key="interact" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="interact" extension_value="false"/>
			<key at="enter" callback="anim_clbk_wanted_item" class_name="movement" param1="needle" param2="hand_l" param3="true"/>
			<key at="exit" callback="anim_clbk_flush_wanted_items" class_name="movement"/>
			<key at="enter" callback="anim_clbk_hide_akimbo_weapon" class_name="movement"/>
			<key at="exit" callback="anim_clbk_show_akimbo_weapon" class_name="movement"/>
		</keys>
		<exit name="upper_body/stand_revive_exit"/>
	</state>

	<state name="upper_body/stand_revive_exit" type="mix" segment="upper_body">
		<anim name="cbt_std_revive_exit_rifle" weight="is_rifle + is_lmg + is_smg + is_shotgun_pump + is_shotgun_mag"/>
		<anim name="cbt_std_revive_exit_pistol" weight="is_pistol + is_revolver"/>
