<!-- Interaction -->
	<state name="upper_body/stand_interact_enter" type="mix" segment="upper_body">
		<anim name="cbt_std_interact_enter_rifle" weight="is_rifle + is_lmg + is_smg + is_shotgun_pump + is_shotgun_mag"/>
		<anim name="cbt_std_interact_enter_pistol" weight="is_pistol + is_revolver"/>
		<anim name="cbt_std_interact_enter_bullpup" weight="is_bullpup + uzi"/>

		<block name="upper_body/std/recoil/auto_exit"/>
		<default blend="5" />
		<keys timebase="relative">
			<key at="enter" set_extension="anim_data" extension_key="interact" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="interact" extension_value="false"/>
			<key at="enter" callback="anim_clbk_hide_akimbo_weapon" class_name="movement"/>
			<key at="exit" callback="anim_clbk_show_akimbo_weapon" class_name="movement"/>
		</keys>
		<exit name="upper_body/stand_interact_loop"/>
	</state>

	<state name="upper_body/stand_interact_loop" type="mixloop" segment="upper_body">
		<anim name="cbt_std_interact_loop_rifle" weight="is_rifle + is_lmg + is_smg + is_shotgun_pump + is_shotgun_mag"/>
		<anim name="cbt_std_interact_loop_pistol" weight="is_pistol + is_revolver"/>
		<anim name="cbt_std_interact_loop_bullpup" weight="is_bullpup + uzi"/>

		<block name="upper_body/std/recoil/auto_exit"/>
		<to name="exit" redirect="upper_body/stand_interact_exit"/>
		<default blend="5" />
		<keys>
			<key at="enter" set_extension="anim_data" extension_key="interact" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="interact" extension_value="false"/>
			<key at="enter" callback="anim_clbk_hide_akimbo_weapon" class_name="movement"/>
			<key at="exit" callback="anim_clbk_show_akimbo_weapon" class_name="movement"/>
		</keys>
