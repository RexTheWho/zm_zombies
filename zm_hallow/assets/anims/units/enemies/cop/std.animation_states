	</state>

	<state name="std/jump/fwd/loop" type="mixloop" segment="base">
		<anim name="cbt_legs_jump_fwd_loop" weight="1" time="1"/>
		<anim name="hos_rifle_idle" weight="hos * rifle" time="1"/>
		<anim name="cbt_rifle_idle" weight="cbt * rifle" time="1"/>
		<anim name="cbt_bullpup_idle" weight="cbt * bullpup" time="1"/>
