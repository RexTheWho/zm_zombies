		<anim name="cbt_akimbo_pistol_idle" weight="cbt * akimbo_pistol" time="1"/>
		<anim name="hos_bow_idle" weight="hos * bow" time="1"/>
		<anim name="cbt_bow_idle" weight="cbt * bow" time="1"/>

		<from_group name="std/jump" blend="3"/>
		<from_group name="std/jump/still/loop" blend="10"/>

		<keys>
			<key at="enter" set_extension="anim_data" extension_key="landing" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="landing" extension_value="false"/>
		</keys>

