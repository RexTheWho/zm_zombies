	<state name="std/crouch/still" type="template_state" segment="base">
		<from_group name="std/crouch/still/turn" blend="19"/>
		<from_group name="std/crouch/move/run/stop" blend="0"/>
		<keys>
			<key at="enter" set_extension="anim_data" extension_key="still" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="still" extension_value="false"/>
		</keys>
	</state>

	<state name="std/crouch/still/idle" type="mixloop" segment="base">
		<from name="std/dodge" blend="19"/>
		<to name="stand" redirect="std/stand/still/std_from_crh"/>

		<anim name="hos_legs_crh_idle" weight="( hos + ntl ) * ( 1 - shield ) * ( 1 - sup )"/>
		<anim name="cbt_legs_crh_idle" weight="cbt * ( 1 - shield ) * ( 1 - sup )"/>
		<anim name="hos_legs_sup_idle" weight="sup * ( hos + ntl ) * ( 1 - shield )"/>
		<anim name="cbt_legs_sup_idle" weight="sup * cbt * ( 1 - shield )"/>

		<anim name="hos_rifle_crh_idle" weight="( hos + ntl ) * rifle * ( 1 - sup ) * (1-vr)"/>
		<anim name="cbt_rifle_crh_idle" weight="cbt * rifle * ( 1 - sup ) * (1-vr)"/>
		<anim name="hos_rifle_sup_idle" weight="( hos + ntl ) * sup * rifle * (1-vr)"/>
		<anim name="cbt_rifle_sup_idle" weight="cbt * sup * rifle * (1-vr)"/>

		<anim name="hos_pistol_crh_idle" weight="( hos + ntl ) * pistol * ( 1 - sup ) * (1-vr)"/>
		<anim name="cbt_pistol_crh_idle" weight="cbt * pistol * ( 1 - shield ) * ( 1 - sup ) * (1-vr)"/>
		<anim name="hos_pistol_sup_idle" weight="( hos + ntl ) * sup * pistol * ( 1 - shield ) * (1-vr)"/>
		<anim name="cbt_pistol_sup_idle" weight="cbt * sup * pistol * ( 1 - shield ) * (1-vr)"/>

		<anim name="cbt_shield_crh_idle" weight="shield"/>

		<anim name="hos_akimbo_pistol_crh_idle" weight="( hos + ntl ) * akimbo_pistol"/>
		<anim name="cbt_akimbo_pistol_crh_idle" weight="cbt * akimbo_pistol"/>
		
		<anim name="hos_bow_crh_idle" weight="( hos + ntl ) * bow * (1-vr)"/>
		<anim name="cbt_bow_crh_idle" weight="cbt * bow * (1-vr)"/>

		<anim name="cbt_bullpup_crh_idle" weight="( hos + ntl + cbt ) * bullpup * (1-vr)"/>

		<!-- VR -->
		<anim name="ntl_rifle_idle_husk1_var1_l" weight="(1-akimbo_pistol) * husk1 * (cbt+hos) * vr" state_length_contrib="false"/>
		<anim name="cbt_rifle_crh_idle_vr" weight="rifle * husk1 * (cbt+hos) * vr"/>
		<anim name="cbt_pistol_crh_idle_vr" weight="pistol * husk1 * (cbt+hos) * vr"/>
		<anim name="cbt_bullpup_crh_idle_vr" weight="bullpup * husk1 * (cbt+hos) * vr"/>
		<!--<anim name="cbt_bow_crh_idle_vr" weight="bow * husk1 * (cbt+hos) * vr"/>-->
		<!-- End VR --> 

		<keys>
			<key at="enter" set_extension="anim_data" extension_key="crouching" extension_value="true"/>
			<key at="full_blend" set_extension="anim_data" extension_key="crouching" extension_value="false"/>
			<key at="exit" set_extension="anim_data" extension_key="crouching" extension_value="false"/>

			<key at="full_blend" set_extension="anim_data" extension_key="idle_full_blend" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="idle_full_blend" extension_value="false"/>

			<key at="enter" set_extension="anim_data" extension_key="base_need_upd" extension_value="true"/>
			<key at="full_blend" set_extension="anim_data" extension_key="base_need_upd" extension_value="false"/>
			<key at="exit" set_extension="anim_data" extension_key="base_need_upd" extension_value="false"/>

			<key at="enter" set_extension="anim_data" extension_key="idle" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="idle" extension_value="false"/>

			<key at="full_blend" set_extension="anim_data" extension_key="can_freeze" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="can_freeze" extension_value="false"/>
		</keys>
	</state>

	<state name="std/crouch/still/crh_from_std" type="mix" segment="base">
		<anim name="cbt_legs_std_to_crh" weight="cbt"/>
		<anim name="hos_legs_std_to_crh" weight="hos + ntl"/>

		<anim name="cbt_rifle_std_to_crh" weight="cbt * rifle * (1-vr)"/>
		<anim name="cbt_bullpup_std_to_crh" weight="cbt * bullpup * (1-vr)"/>
		<anim name="cbt_pistol_std_to_crh" weight="cbt * pistol * (1-vr)"/>
		<anim name="cbt_akimbo_pistol_std_to_crh" weight="cbt * akimbo_pistol"/>

		<anim name="hos_rifle_std_to_crh" weight="( hos + ntl ) * rifle * (1-vr)"/>
		<anim name="hos_pistol_std_to_crh" weight="( hos + ntl ) * pistol * (1-vr)"/>
		<anim name="hos_akimbo_pistol_std_to_crh" weight="( hos + ntl ) * akimbo_pistol"/>
		
		<anim name="cbt_bow_std_to_crh" weight="cbt * bow * (1-vr)"/>
		<anim name="hos_bow_std_to_crh" weight="( hos + ntl ) * bow * (1-vr)"/>

		<!-- VR -->
		<anim name="ntl_rifle_idle_husk1_var1_l" weight="(1-akimbo_pistol) * husk1 * (cbt+hos) * vr" state_length_contrib="false"/>
		<anim name="cbt_rifle_std_to_crh_vr" weight="rifle * husk1 * (cbt+hos) * vr"/>
		<anim name="cbt_pistol_std_to_crh_vr" weight="pistol * husk1 * (cbt+hos) * vr"/>
		<anim name="cbt_bullpup_std_to_crh_vr" weight="bullpup * husk1 * (cbt+hos) * vr"/>
		<!--<anim name="cbt_bow_std_to_crh_vr" weight="bow * husk1 * (cbt+hos) * vr"/>-->
		<!-- End VR --> 

		<keys>
			<key at="enter" set_extension="anim_data" extension_key="look" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="look" extension_value="false"/>
			<key at="enter" set_extension="anim_data" extension_key="base_no_recoil" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="base_no_recoil" extension_value="false"/>

			<key at="full_blend" set_extension="anim_data" extension_key="can_freeze" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="can_freeze" extension_value="false"/>
		</keys>
		<exit name="std/crouch/still/idle"/>
	</state>

	<state name="std/crouch/still/turn" type="template_state" segment="base">
		<keys>
			<key at="enter" set_extension="anim_data" extension_key="turn" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="turn" extension_value="false"/>
		</keys>
		<exit name="std/crouch/still/idle"/>
	</state>

	<state name="std/crouch/still/turn/l" type="mix" segment="base">
		<param name="angle45" value="0"/>
		<param name="angle90" value="0"/>
		<param name="angle135" value="0"/>

		<anim name="hos_legs_crh_turn_l_45" weight="( hos + ntl ) * ( 1 - shield ) * ( 1 - sup ) * angle45"/>
		<anim name="hos_legs_crh_turn_l_90" weight="( hos + ntl ) * ( 1 - shield ) * ( 1 - sup ) * angle90"/>
		<anim name="hos_legs_crh_turn_l_135" weight="( hos + ntl ) * ( 1 - shield ) * ( 1 - sup ) * angle135"/>

		<anim name="hos_legs_sup_turn_l_45" weight="( hos + ntl ) * sup * angle45"/>
		<anim name="hos_legs_sup_turn_l_90" weight="( hos + ntl ) * sup * angle90"/>
		<anim name="hos_legs_sup_turn_l_135" weight="( hos + ntl ) * sup * angle135"/>

		<anim name="cbt_legs_crh_turn_l_45" weight="cbt * ( 1 - shield ) * ( 1 - sup ) * angle45"/>
		<anim name="cbt_legs_crh_turn_l_90" weight="cbt * ( 1 - shield ) * ( 1 - sup ) * angle90"/>
		<anim name="cbt_legs_crh_turn_l_135" weight="cbt * ( 1 - shield ) * ( 1 - sup ) * angle135"/>

		<anim name="cbt_legs_sup_turn_l_45" weight="cbt * sup * angle45"/>
		<anim name="cbt_legs_sup_turn_l_90" weight="cbt * sup * angle90"/>
		<anim name="cbt_legs_sup_turn_l_135" weight="cbt * sup * angle135"/>

		<anim name="hos_rifle_crh_turn_l_45" weight="( hos + ntl ) * ( 1 - sup ) * rifle * angle45 * (1-vr)"/>
		<anim name="hos_rifle_crh_turn_l_90" weight="( hos + ntl ) * ( 1 - sup ) * rifle * angle90 * (1-vr)"/>
		<anim name="hos_rifle_crh_turn_l_135" weight="( hos + ntl ) * ( 1 - sup ) * rifle * angle135 * (1-vr)"/>

		<anim name="hos_rifle_sup_turn_l_45" weight="( hos + ntl ) * sup * rifle * angle45 * (1-vr)"/>
		<anim name="hos_rifle_sup_turn_l_90" weight="( hos + ntl ) * sup * rifle * angle90 * (1-vr)"/>
		<anim name="hos_rifle_sup_turn_l_135" weight="( hos + ntl ) * sup * rifle * angle135 * (1-vr)"/>

		<anim name="cbt_rifle_crh_turn_l_45" weight="cbt * ( 1 - sup ) * rifle * angle45 * (1-vr)"/>
		<anim name="cbt_rifle_crh_turn_l_90" weight="cbt * ( 1 - sup ) * rifle * angle90 * (1-vr)"/>
		<anim name="cbt_rifle_crh_turn_l_135" weight="cbt * ( 1 - sup ) * rifle * angle135 * (1-vr)"/>

		<anim name="cbt_rifle_sup_turn_l_45" weight="cbt * sup * rifle * angle45 * (1-vr)"/>
		<anim name="cbt_rifle_sup_turn_l_90" weight="cbt * sup * rifle * angle90 * (1-vr)"/>
		<anim name="cbt_rifle_sup_turn_l_135" weight="cbt * sup * rifle * angle135 * (1-vr)"/>

		<anim name="hos_pistol_crh_turn_l_45" weight="( hos + ntl ) * ( 1 - sup ) * pistol * angle45 * (1-vr)"/>
		<anim name="hos_pistol_crh_turn_l_90" weight="( hos + ntl ) * ( 1 - sup ) * pistol * angle90 * (1-vr)"/>
		<anim name="hos_pistol_crh_turn_l_135" weight="( hos + ntl ) * ( 1 - sup ) * pistol * angle135 * (1-vr)"/>

		<anim name="hos_pistol_sup_turn_l_45" weight="( hos + ntl ) * sup * pistol * angle45 * (1-vr)"/>
		<anim name="hos_pistol_sup_turn_l_90" weight="( hos + ntl ) * sup * pistol * angle90 * (1-vr)"/>
		<anim name="hos_pistol_sup_turn_l_135" weight="( hos + ntl ) * sup * pistol * angle135 * (1-vr)"/>

		<anim name="cbt_pistol_crh_turn_l_45" weight="cbt * ( 1 - sup ) * ( 1 - shield ) * pistol * angle45 * (1-vr)"/>
		<anim name="cbt_pistol_crh_turn_l_90" weight="cbt * ( 1 - sup ) * ( 1 - shield ) * pistol * angle90 * (1-vr)"/>
		<anim name="cbt_pistol_crh_turn_l_135" weight="cbt * ( 1 - sup ) * ( 1 - shield ) * pistol * angle135 * (1-vr)"/>

		<anim name="cbt_pistol_sup_turn_l_45" weight="cbt * sup * pistol * angle45 * (1-vr)"/>
		<anim name="cbt_pistol_sup_turn_l_90" weight="cbt * sup * pistol * angle90 * (1-vr)"/>
		<anim name="cbt_pistol_sup_turn_l_135" weight="cbt * sup * pistol * angle135 * (1-vr)"/>

		<anim name="cbt_shield_crh_turn_l_45" weight="shield * angle45"/>
		<anim name="cbt_shield_crh_turn_l_90" weight="shield * angle90"/>
		<anim name="cbt_shield_crh_turn_l_135" weight="shield * angle135"/>

		<anim name="hos_akimbo_pistol_crh_turn_l_45" weight="( hos + ntl ) * akimbo_pistol * angle45"/>
		<anim name="hos_akimbo_pistol_crh_turn_l_90" weight="( hos + ntl ) * akimbo_pistol * angle90"/>
		<anim name="hos_akimbo_pistol_crh_turn_l_135" weight="( hos + ntl ) * akimbo_pistol * angle135"/>

		<anim name="cbt_akimbo_pistol_crh_turn_l_45" weight="cbt * akimbo_pistol * angle45"/>
		<anim name="cbt_akimbo_pistol_crh_turn_l_90" weight="cbt * akimbo_pistol * angle90"/>
		<anim name="cbt_akimbo_pistol_crh_turn_l_135" weight="cbt * akimbo_pistol * angle135"/>
		
		<anim name="hos_bow_crh_turn_l_45" weight="( hos + ntl ) * bow * angle45 * (1-vr)"/>
		<anim name="hos_bow_crh_turn_l_90" weight="( hos + ntl ) * bow * angle90 * (1-vr)"/>
		<anim name="hos_bow_crh_turn_l_135" weight="( hos + ntl ) * bow * angle135 * (1-vr)"/>

		<anim name="cbt_bow_crh_turn_l_45" weight="cbt * bow * angle45 * (1-vr)"/>
		<anim name="cbt_bow_crh_turn_l_90" weight="cbt * bow * angle90 * (1-vr)"/>
		<anim name="cbt_bow_crh_turn_l_135" weight="cbt * bow * angle135 * (1-vr)"/>

		<anim name="cbt_bullpup_crh_turn_l_45" weight="(ntl + hos + cbt) * bullpup * angle45"/>

		<!-- VR -->
		<anim name="ntl_rifle_idle_husk1_var1_l" weight="(1-akimbo_pistol) * husk1 * (cbt+hos) * vr" state_length_contrib="false"/>
		<anim name="cbt_rifle_crh_turn_l_45_vr" weight="rifle * husk1 * (cbt+hos) * vr * angle45"/>
		<anim name="cbt_rifle_crh_turn_l_90_vr" weight="rifle * husk1 * (cbt+hos) * vr * angle90"/>
		<anim name="cbt_rifle_crh_turn_l_135_vr" weight="rifle * husk1 * (cbt+hos) * vr * angle135"/>
		<anim name="cbt_pistol_crh_turn_l_45_vr" weight="pistol * husk1 * (cbt+hos) * vr * angle45"/>
		<anim name="cbt_pistol_crh_turn_l_90_vr" weight="pistol * husk1 * (cbt+hos) * vr * angle90"/>
		<anim name="cbt_pistol_crh_turn_l_135_vr" weight="pistol * husk1 * (cbt+hos) * vr * angle135"/>
		<anim name="cbt_bullpup_crh_turn_l_45_vr" weight="bullpup * husk1 * (cbt+hos) * vr * angle45"/>
		<!-- <anim name="cbt_bullpup_crh_turn_l_90_vr" weight="bullpup * husk1 * (cbt+hos) * vr"/> -->
		<!-- <anim name="cbt_bullpup_crh_turn_l_135_vr" weight="bullpup * husk1 * (cbt+hos) * vr"/> -->
		<!--<anim name="cbt_bow_crh_turn_l_45_vr" weight="bow * husk1 * (cbt+hos) * vr"/>-->
