	<state name="upper_body/stand_reload" type="mix" segment="upper_body" mute="upper_body_ext">

		<param name="hold_pistol" value="0"/>
		<param name="hold_rifle" value="0"/>
		<param name="hold_bullpup" value="0"/>
		<param name="hold_uzi" value="0"/>
		<param name="hold_akimbo_pistol" value="0"/>
		<param name="hold_bow" value="0"/>

		<param name="reload_pistol" value="0"/>
		<param name="reload_revolver" value="0"/>
		<param name="reload_rifle" value="0"/>
		<param name="reload_bullpup" value="0"/>
		<param name="reload_uzi" value="0"/>
		<param name="reload_akimbo_pistol" value="0"/>
		<param name="reload_bow" value="0"/>
		<param name="reload_overgrill" value="0"/>

		<!-- New player reloads -->
		<anim name="stand_reload_beretta" weight="(1-is_npc) * (hold_pistol * reload_pistol)"/>  <!-- Pistol hold, reload -->
		<anim name="cbt_std_revolver_reload" weight="(1-is_npc) * (hold_pistol * reload_revolver)"/>  <!-- Pistol hold, revolver reload -->

		<anim name="stand_reload_m4" weight="(1-is_npc) * (hold_rifle * reload_rifle)"/> <!-- Rifle hold, reload -->
		<anim name="stand_reload_m4_uzi" weight="(1-is_npc) * (hold_rifle * reload_uzi)"/> <!-- Rifle hold, uzi reload -->
		<anim name="stand_reload_m4_bullpup" weight="(1-is_npc) * (hold_rifle * reload_bullpup)"/> <!-- Rifle hold, bullpup reload -->

		<anim name="stand_reload_bullpup" weight="(1-is_npc) * (hold_bullpup * reload_bullpup)"/> <!-- Bullpup hold, reload -->
		<anim name="stand_reload_bullpup_rifle" weight="(1-is_npc) * (hold_bullpup * reload_rifle)"/> <!-- Bullpup hold, rifle reload -->
		<anim name="stand_reload_bullpup_overgrill" weight="(1-is_npc) * (hold_bullpup * reload_overgrill)"/> <!-- Bullpup hold, overgrill reload -->

		<anim name="stand_reload_uzi" weight="(1-is_npc) * (hold_uzi * reload_uzi)"/> <!-- Bullpup hold, uzi reload -->

		<anim name="stand_akimbo_pistol_reload" weight="(1-is_npc) * (hold_akimbo_pistol * reload_akimbo_pistol)"/> <!-- Akimbo hold, reload -->

		<anim name="stand_bow_reload" weight="(1-is_npc) * (hold_bow * reload_bow)"/> <!-- Bow hold, reload -->

		<!-- NPC reloads -->
		<anim name="stand_reload_beretta" weight="is_npc * is_pistol"/>
		<anim name="cbt_std_revolver_reload" weight="is_npc * is_revolver"/>
		<anim name="stand_reload_m4" weight="is_npc * ( 1 - tank ) * ( is_rifle + is_lmg + (is_smg * (1 - uzi)) + is_shotgun_mag )"/>
		<anim name="stand_reload_bullpup" weight="is_npc * ( 1 - tank ) * ( is_bullpup )"/>
		<anim name="stand_reload_uzi" weight="is_npc * ( 1 - tank ) * ( is_smg ) * uzi"/>
		<anim name="stand_reload_r870" weight="is_npc * ( 1 - tank ) * ( is_shotgun_pump )"/>
		<anim name="stand_akimbo_pistol_reload" weight="is_npc * akimbo_pistol"/>
		<anim name="stand_bow_reload" weight="is_npc * bow"/>

		<anim name="stand_reload_m4" weight="is_npc * tank * is_smg"/>
		<anim name="stand_reload_m4" weight="is_npc * tank * is_shotgun_mag"/>
		<anim name="tank_reload_shotgun" weight="is_npc * tank * is_shotgun_pump"/>
		<anim name="tank_reload_lmg" weight="is_npc * tank * (is_rifle + mini)"/>

		<block name="upper_body/std/recoil/auto_exit"/>

		<default blend="5" />
		<keys timebase="frames">
			<key at="enter" set_extension="anim_data" extension_key="reload" extension_value="true"/>
			<key at="trigger" trigger="grab_mag" callback="anim_clbk_show_magazine_in_hand" class_name="movement"/>
			<key at="trigger" trigger="drop_mag" callback="anim_clbk_spawn_dropped_magazine" class_name="movement"/>
			<key at="trigger" trigger="new_mag" callback="anim_clbk_show_new_magazine_in_hand" class_name="movement"/>
			<key at="trigger" trigger="attach_mag" callback="anim_clbk_hide_magazine_in_hand" class_name="movement"/>
			<key at="exit" set_extension="anim_data" extension_key="reload" extension_value="false"/>
			<key at="exit" callback="anim_clbk_reload_exit" class_name="movement"/>

			<key at="trigger" trigger="wp_3p_rifle_gen_clip_out_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_rifle_gen_clip_out"/>
			<key at="trigger" trigger="wp_3p_rifle_gen_clip_in_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_rifle_gen_clip_in"/>
			<key at="trigger" trigger="wp_3p_rifle_gen_clip_out_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_rifle_gen_clip_out"/>
			<key at="trigger" trigger="wp_3p_rifle_gen_clip_in_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_rifle_gen_clip_in"/>
			<key at="trigger" trigger="wp_3p_rifle_gen_clip_out_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_rifle_gen_clip_out"/>
			<key at="trigger" trigger="wp_3p_rifle_gen_clip_in_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_rifle_gen_clip_in"/>
			<key at="trigger" trigger="wp_3p_gun_gen_mag_out_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_gun_gen_mag_out"/>
			<key at="trigger" trigger="wp_3p_gun_gen_mag_in_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_gun_gen_mag_in"/>
			<key at="trigger" trigger="wp_3p_revolver_gen_shells_out_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_revolver_gen_shells_out"/>
			<key at="trigger" trigger="wp_3p_revolver_gen_shells_in_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_revolver_gen_shells_in"/>
