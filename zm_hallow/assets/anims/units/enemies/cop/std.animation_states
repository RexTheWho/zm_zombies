	<state name="upper_body/crouch_reload" type="mix" segment="upper_body" mute="upper_body_ext">

		<param name="hold_pistol" value="0"/>
		<param name="hold_rifle" value="0"/>
		<param name="hold_bullpup" value="0"/>
		<param name="hold_uzi" value="0"/>
		<param name="hold_akimbo_pistol" value="0"/>
		<param name="hold_bow" value="0"/>

		<param name="reload_pistol" value="0"/>
		<param name="reload_revolver" value="0"/>
		<param name="reload_rifle" value="0"/>
		<param name="reload_bullpup" value="0"/>
		<param name="reload_uzi" value="0"/>
		<param name="reload_akimbo_pistol" value="0"/>
		<param name="reload_bow" value="0"/>

		<!-- New player reloads -->
		<anim name="crh_reload_beretta" weight="(1-is_npc) * (hold_pistol * reload_pistol)"/>  <!-- Pistol hold, reload -->
		<anim name="cbt_crh_revolver_reload" weight="(1-is_npc) * (hold_pistol * reload_revolver)"/>  <!-- Pistol hold, revolver reload -->

		<anim name="crh_reload_m4" weight="(1-is_npc) * (hold_rifle * reload_rifle)"/> <!-- Rifle hold, reload -->
		<anim name="crh_reload_m4_uzi" weight="(1-is_npc) * (hold_rifle * reload_uzi)"/> <!-- Rifle hold, uzi reload -->
		<anim name="crh_reload_m4_bullpup" weight="(1-is_npc) * (hold_rifle * reload_bullpup)"/> <!-- Rifle hold, bullpup reload -->

		<anim name="crh_reload_bullpup" weight="(1-is_npc) * (hold_bullpup * reload_bullpup)"/> <!-- Bullpup hold, reload -->
		<anim name="crh_reload_bullpup_rifle" weight="(1-is_npc) * (hold_bullpup * reload_rifle)"/> <!-- Bullpup hold, rifle reload -->

		<anim name="crh_reload_uzi" weight="(1-is_npc) * (hold_uzi * reload_uzi)"/> <!-- Bullpup hold, uzi reload -->

		<anim name="crh_akimbo_pistol_reload" weight="(1-is_npc) * (hold_akimbo_pistol * reload_akimbo_pistol)"/> <!-- Akimbo hold, reload -->

		<anim name="crh_bow_reload" weight="(1-is_npc) * (hold_bow * reload_bow)"/> <!-- Bow hold, reload -->

		<!-- NPC reloads -->
		<anim name="crh_reload_beretta" weight="is_npc * is_pistol * (1 - shield) * ( 1 - sup )"/>
		<anim name="cbt_crh_revolver_reload" weight="is_npc * is_revolver * (1 - shield) * ( 1 - sup )"/>
		<anim name="crh_reload_m4" weight="is_npc * is_rifle * ( 1 - sup )" />
		<anim name="crh_reload_r870" weight="is_npc * is_shotgun_pump  * ( 1 - sup )" />
		<anim name="crh_reload_mp5" weight="is_npc * (is_smg * (1 - uzi)) * ( 1 - sup ) * (1 - shield)" />
		<anim name="crh_reload_shield" weight="is_npc * shield" />
		<anim name="sup_reload_beretta" weight="is_npc * is_pistol * sup" />
		<anim name="cbt_sup_revolver_reload" weight="is_npc * is_revolver * sup" />
		<anim name="sup_reload_m4" weight="is_npc * is_rifle * sup" />
		<anim name="sup_reload_mp5" weight="is_npc * (is_smg * (1 - uzi)) * sup" />
		<anim name="sup_reload_shotgun" weight="is_npc * is_shotgun_pump * sup" />

		<anim name="crh_akimbo_pistol_reload" weight="is_npc * akimbo_pistol"/>
		<anim name="crh_bow_reload" weight="is_npc * bow"/>
		<anim name="crh_reload_bullpup" weight="is_npc * is_bullpup"/>
		<anim name="crh_reload_uzi" weight="is_npc * is_smg * uzi"/>

		<block name="upper_body/crh/recoil/auto_exit"/>

		<default blend="5" />
		<keys timebase="frames">
			<key at="enter" set_extension="anim_data" extension_key="reload" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="reload" extension_value="false"/>
			<key at="enter" set_extension="anim_data" extension_key="switch_weapon" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="switch_weapon" extension_value="false"/>
			<key at="trigger" trigger="grab_mag" callback="anim_clbk_show_magazine_in_hand" class_name="movement"/>
			<key at="trigger" trigger="drop_mag" callback="anim_clbk_spawn_dropped_magazine" class_name="movement"/>
			<key at="trigger" trigger="new_mag" callback="anim_clbk_show_new_magazine_in_hand" class_name="movement"/>
			<key at="trigger" trigger="attach_mag" callback="anim_clbk_hide_magazine_in_hand" class_name="movement"/>
			<key at="exit" callback="anim_clbk_reload_exit" class_name="movement"/>

			<key at="trigger" trigger="wp_3p_rifle_gen_clip_out_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_rifle_gen_clip_out"/>
			<key at="trigger" trigger="wp_3p_rifle_gen_clip_in_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_rifle_gen_clip_in"/>
			<key at="trigger" trigger="wp_3p_rifle_gen_clip_out_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_rifle_gen_clip_out"/>
			<key at="trigger" trigger="wp_3p_rifle_gen_clip_in_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_rifle_gen_clip_in"/>
			<key at="trigger" trigger="wp_3p_rifle_gen_clip_out_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_rifle_gen_clip_out"/>
			<key at="trigger" trigger="wp_3p_rifle_gen_clip_in_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_rifle_gen_clip_in"/>
			<key at="trigger" trigger="wp_3p_gun_gen_mag_out_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_gun_gen_mag_out"/>
			<key at="trigger" trigger="wp_3p_gun_gen_mag_in_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_gun_gen_mag_in"/>
			<key at="trigger" trigger="wp_3p_revolver_gen_shells_out_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_revolver_gen_shells_out"/>
			<key at="trigger" trigger="wp_3p_revolver_gen_shells_in_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_revolver_gen_shells_in"/>
			<key at="trigger" trigger="wp_3p_revolver_gen_cylinder_in_01" callback="_play_weapon_reload_animation_sfx" class_name="movement" param1="wp_3p_revolver_gen_cylinder_in"/>
		</keys>
		<exit name="upper_body/empty"/>
	</state>

