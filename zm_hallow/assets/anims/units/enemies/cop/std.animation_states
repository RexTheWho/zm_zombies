	<state name="std/stand" type="template_state" segment="base">
		<from_group name="std/crouch" blend="24"/>
		<from name="std/stand/still/land" blend="10"/>
		<from name="std/stand/move/walk/fwd/land" blend="15"/>
		<from name="std/stand/move/walk/bwd/land" blend="10"/>
		<from name="std/stand/move/run/fwd/land" blend="12"/>
		<from name="std/stand/move/run/bwd/land" blend="10"/>
		<from name="std/stand/move/run/sprint/fwd/land" blend="10"/>
		<from name="std/stand/move/run/sprint/bwd/land" blend="10"/>
		<keys>
			<key at="enter" set_extension="anim_data" extension_key="stand" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="stand" extension_value="false"/>
			<key at="enter" set_extension="anim_data" extension_key="pose" extension_value="stand"/>
			<key at="exit" set_extension="anim_data" extension_key="pose" extension_value="false"/>
		</keys>
	</state>

	<state name="std/stand/still" type="template_state" segment="base">
		<from_group name="std/stand/move/run/stop" blend="0"/>
		<to name="crouch" redirect="std/crouch/still/crh_from_std"/>
		<keys>
			<key at="enter" set_extension="anim_data" extension_key="still" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="still" extension_value="false"/>
		</keys>
	</state>

	<state name="std/stand/still/attached_collar/enter" type="once" segment="base">
		<anim name="attached_collar_enter"/>

		<keys>
			<key at="enter" set_extension="anim_data" extension_key="act" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="act" extension_value="false"/>

			<key at="enter" set_extension="anim_data" extension_key="base_need_upd" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="base_need_upd" extension_value="false"/>
		</keys>
		<exit name="std/stand/still/idle/look"/>
	</state>

	<state name="std/stand/still/std_from_crh" type="mix" segment="base">
		<anim name="cbt_legs_crh_to_std" weight="cbt"/>
		<anim name="hos_legs_crh_to_std" weight="hos"/>

		<anim name="cbt_rifle_crh_to_std" weight="cbt * rifle * (1-vr)"/>
		<anim name="cbt_bullpup_crh_to_std" weight="cbt * bullpup * (1-vr)"/>
		<anim name="cbt_pistol_crh_to_std" weight="cbt * pistol * (1-vr)"/>
		<anim name="cbt_akimbo_pistol_crh_to_std" weight="cbt * akimbo_pistol"/>
		<anim name="cbt_bow_crh_to_std" weight="cbt * bow * (1-vr)"/>

		<anim name="hos_rifle_crh_to_std" weight="hos * rifle * (1-vr)"/>
		<anim name="hos_pistol_crh_to_std" weight="hos * pistol * (1-vr)"/>
		<anim name="hos_akimbo_pistol_crh_to_std" weight="hos * akimbo_pistol"/>
		<anim name="hos_bow_crh_to_std" weight="hos * bow * (1-vr)"/>
		
		<!-- VR -->
		<anim name="ntl_rifle_idle_husk1_var1_l" weight="(1-akimbo_pistol) * husk1 * (cbt+hos) * vr" state_length_contrib="false"/>
		<anim name="cbt_rifle_crh_to_std_vr" weight="rifle * husk1 * (cbt+hos) * vr"/>
		<anim name="cbt_pistol_crh_to_std_vr" weight="pistol * husk1 * (cbt+hos) * vr"/>
		<anim name="cbt_bullpup_crh_to_std_vr" weight="bullpup * husk1 * (cbt+hos) * vr"/>
		<!--<anim name="cbt_bow_crh_to_std_vr" weight="bow * husk1 * (cbt+hos) * vr"/>-->
		<!-- End VR --> 

		<keys>
			<key at="enter" set_extension="anim_data" extension_key="base_no_recoil" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="base_no_recoil" extension_value="false"/>
		</keys>

		<exit name="std/stand/still/idle/look"/>
	</state>

	<state name="std/stand/still/idle" type="template_state" segment="base">
		<from_group name="std/stand/still/turn" blend="10"/>
		<keys>
			<key at="enter" set_extension="anim_data" extension_key="idle" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="idle" extension_value="false"/>

			<key at="enter" set_extension="anim_data" extension_key="base_need_upd" extension_value="true"/>
			<key at="full_blend" set_extension="anim_data" extension_key="base_need_upd" extension_value="false"/>
			<key at="exit" set_extension="anim_data" extension_key="base_need_upd" extension_value="false"/>

			<key at="full_blend" set_extension="anim_data" extension_key="idle_full_blend" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="idle_full_blend" extension_value="false"/>
		</keys>
	</state>

	<state name="std/stand/still/idle/look" type="mixloop" segment="base">
		<from_group name="std/spawn" blend="0"/>
		<from_group name="std/stand/still/turn" blend="10"/>
		<from_group name="std/stand/still/spooc" blend="9"/>

		<to name="stand" redirect="std/stand/still"/>

		<anim name="ntl_legs_idle" weight="cop * ntl"/>
		<anim name="ntl_legs_idle_husk1_var1" weight="husk1 * ntl"/>
		<anim name="ntl_legs_idle_husk2_var1" weight="husk2 * ntl"/>
		<anim name="ntl_legs_idle_husk3_var1" weight="husk3 * ntl"/>
		<anim name="ntl_legs_idle_husk4_var1" weight="husk4 * ntl"/>
		<anim name="hos_legs_idle" weight="( cop + husk1 + husk2 + husk3 + husk4 ) * hos"/>
		<anim name="cbt_legs_idle" weight="( cop + husk1 + husk2 + husk3 + husk4 ) * cbt"/>
		<anim name="tank_legs_idle" weight="tank"/>

		<anim name="ntl_rifle_idle" weight="cop * ntl * rifle"/>
		<anim name="ntl_rifle_idle_husk1_var1" weight="husk1 * ntl * rifle"/>
		<anim name="ntl_rifle_idle_husk2_var1" weight="husk2 * ntl * rifle"/>
		<anim name="ntl_rifle_idle_husk3_var1" weight="husk3 * ntl * rifle"/>
		<anim name="ntl_rifle_idle_husk4_var1" weight="husk4 * ntl * rifle"/>
		<anim name="hos_rifle_idle" weight="( cop + husk1 + husk2 + husk3 + husk4 ) * hos * rifle * (1-vr)"/>
		<anim name="cbt_rifle_idle" weight="( cop + husk1 + husk2 + husk3 + husk4 ) * cbt * rifle * (1-vr)"/>
		<anim name="cbt_bullpup_idle" weight="( cop + husk1 + husk2 + husk3 + husk4 ) * (hos + cbt) * bullpup * (1-vr)"/>
		<anim name="tank_idle" weight="tank"/>

		<anim name="ntl_pistol_idle" weight="cop * ntl * pistol"/>
		<anim name="ntl_pistol_idle_husk1_var1" weight="husk1 * ntl * (pistol+akimbo_pistol+bow)"/>
		<anim name="ntl_pistol_idle_husk2_var1" weight="husk2 * ntl * (pistol+akimbo_pistol+bow)"/>
		<anim name="ntl_pistol_idle_husk3_var1" weight="husk3 * ntl * (pistol+akimbo_pistol+bow)"/>
		<anim name="ntl_pistol_idle_husk4_var1" weight="husk4 * ntl * (pistol+akimbo_pistol+bow)"/>
		<anim name="hos_pistol_idle" weight="( cop + husk1 + husk2 + husk3 + husk4 ) * hos * pistol * (1-vr)"/>
		<anim name="cbt_pistol_idle" weight="( cop + husk1 + husk2 + husk3 + husk4 ) * cbt * pistol * (1-vr)"/>

		<anim name="hos_akimbo_pistol_idle" weight="( cop + husk1 + husk2 + husk3 + husk4 ) * hos * akimbo_pistol"/>
		<anim name="cbt_akimbo_pistol_idle" weight="( cop + husk1 + husk2 + husk3 + husk4 ) * cbt * akimbo_pistol"/>
		
		<anim name="hos_bow_idle" weight="( cop + husk1 + husk2 + husk3 + husk4 ) * hos * bow * (1-vr)"/>
		<anim name="cbt_bow_idle" weight="( cop + husk1 + husk2 + husk3 + husk4 ) * cbt * bow * (1-vr)"/>

		<!-- VR -->
		<anim name="ntl_rifle_idle_husk1_var1_l" weight="(1-akimbo_pistol) * husk1 * (cbt+hos) * vr" state_length_contrib="false"/>
		<anim name="cbt_rifle_idle_vr" weight="rifle * husk1 * (cbt+hos) * vr"/>
		<anim name="cbt_pistol_idle_vr" weight="pistol * husk1 * (cbt+hos) * vr"/>
		<anim name="cbt_bullpup_idle_vr" weight="bullpup * husk1 * (cbt+hos) * vr"/>
		<!--<anim name="cbt_bow_idle_vr" weight="bow * husk1 * (cbt+hos) * vr"/>-->
		<!-- End VR --> 

		<keys>
			<key at="enter" set_extension="anim_data" extension_key="look" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="look" extension_value="false"/>

			<key at="full_blend" set_extension="anim_data" extension_key="can_freeze" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="can_freeze" extension_value="false"/>
		</keys>
	</state>

