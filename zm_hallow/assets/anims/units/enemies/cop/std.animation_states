		<anim name="stand_r870_recoil" weight="(1 - tank) * is_shotgun_pump"/>
		<anim name="tank_recoil_saiga" weight="tank * ( is_shotgun_mag + is_rifle + mini )"/>
		<anim name="tank_recoil_shotgun" weight="tank * is_shotgun_pump"/>
	</state>

	<state name="upper_body/crh/recoil" type="template_state" segment="upper_body">
