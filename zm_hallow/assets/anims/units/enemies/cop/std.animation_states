	<state name="std/crouch/move/walk/fwd" type="mixloop" segment="base">
		<from_group name="std/special/to_idle" blend="19"/>
		<anim name="hos_legs_crh_walk_fwd" weight="(hos + ntl) * (1 - sup)"/>
		<anim name="cbt_legs_crh_walk_fwd" weight="cbt * (1 - sup) * (1-shield)"/>
		<anim name="hos_legs_sup_walk_fwd" weight="hos * sup"/>
		<anim name="cbt_legs_sup_walk_fwd" weight="cbt * sup * (1-shield)"/>

		<anim name="hos_rifle_crh_walk_fwd" weight="( hos + ntl ) * (1 - sup) * rifle * (1-vr)"/>
		<anim name="cbt_rifle_crh_walk_fwd" weight="cbt * (1 - sup) * rifle * (1-vr)"/>

		<anim name="hos_pistol_crh_walk_fwd" weight="( hos + ntl ) * (1 - sup) * pistol * (1-vr)"/>
		<anim name="cbt_pistol_crh_walk_fwd" weight="cbt * (1 - sup) * pistol * (1-shield) * (1-vr)"/>

		<anim name="hos_akimbo_pistol_crh_walk_fwd" weight="( hos + ntl ) * akimbo_pistol"/>
		<anim name="cbt_akimbo_pistol_crh_walk_fwd" weight="cbt * akimbo_pistol"/>
		
		<anim name="hos_bow_crh_walk_fwd" weight="( hos + ntl ) * bow * (1-vr)"/>
		<anim name="cbt_bow_crh_walk_fwd" weight="cbt * bow * (1-vr)"/>

		<anim name="cbt_bullpup_crh_walk_fwd" weight="( cbt + hos + ntl ) * bullpup * (1-vr)"/>

		<anim name="hos_rifle_sup_walk_fwd" weight="(hos + ntl) * sup * (1 - wnd) * rifle * (1-vr)"/>
		<anim name="cbt_rifle_sup_walk_fwd" weight="cbt * sup * rifle * (1-vr)"/>
		<anim name="hos_pistol_sup_walk_fwd" weight="(hos + ntl) * sup * (1 - wnd) * pistol * (1-vr)"/>
		<anim name="cbt_pistol_sup_walk_fwd" weight="cbt * sup * pistol * (1-shield) * (1-vr)"/>

		<anim name="cbt_shield_crh_walk_fwd" weight="shield"/>

		<!-- VR --> 
		<anim name="ntl_rifle_idle_husk1_var1_l" weight="(1-akimbo_pistol) * husk1 * (cbt+hos) * vr" state_length_contrib="false"/>
		<anim name="cbt_rifle_crh_walk_fwd_vr" weight="rifle * husk1 * (cbt+hos) * vr"/>
		<anim name="cbt_pistol_crh_walk_fwd_vr" weight="pistol * husk1 * (cbt+hos) * vr"/>
		<anim name="cbt_bullpup_crh_walk_fwd_vr" weight="bullpup * husk1 * (cbt+hos) * vr"/>
		<!--<anim name="cbt_bow_crh_walk_fwd_vr" weight="bow * husk1 * (cbt+hos) * vr"/>-->
		<!-- End VR -->

		<to name="stand" redirect="std/stand/move/walk/fwd"/>
		<keys>
			<key at="enter" set_extension="anim_data" extension_key="move_fwd" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="move_fwd" extension_value="false"/>
			<key at="enter" set_extension="anim_data" extension_key="move_side" extension_value="fwd"/>
			<key at="exit" set_extension="anim_data" extension_key="move_side" extension_value="false"/>
		</keys>
	</state>

	<state name="std/crouch/move/walk/bwd" type="mixloop" segment="base">
		<anim name="hos_legs_crh_walk_bwd" weight="(hos + ntl) * (1 - sup)"/>
		<anim name="cbt_legs_crh_walk_bwd" weight="cbt * (1 - sup) * (1-shield)"/>
		<anim name="hos_legs_sup_walk_bwd" weight="hos * sup"/>
		<anim name="cbt_legs_sup_walk_bwd" weight="cbt * sup * (1-shield)"/>

		<anim name="hos_rifle_crh_walk_bwd" weight="( hos + ntl ) * (1 - sup) * rifle * (1-vr)"/>
		<anim name="cbt_rifle_crh_walk_bwd" weight="cbt * (1 - sup) * rifle * (1-vr)"/>

		<anim name="hos_pistol_crh_walk_bwd" weight="( hos + ntl ) * (1 - sup) * pistol * (1-vr)"/>
		<anim name="cbt_pistol_crh_walk_bwd" weight="cbt * (1 - sup) * pistol * (1-shield) * (1-vr)"/>

		<anim name="hos_akimbo_pistol_crh_walk_bwd" weight="( hos + ntl ) * akimbo_pistol"/>
		<anim name="cbt_akimbo_pistol_crh_walk_bwd" weight="cbt * akimbo_pistol"/>
		
		<anim name="hos_bow_crh_walk_bwd" weight="( hos + ntl ) * bow * (1-vr)"/>
		<anim name="cbt_bow_crh_walk_bwd" weight="cbt * bow * (1-vr)"/>

		<anim name="cbt_bullpup_crh_walk_bwd" weight="( cbt + hos + ntl ) * bullpup * (1-vr)"/>

		<anim name="hos_rifle_sup_walk_bwd" weight="(hos + ntl) * sup * (1 - wnd) * rifle * (1-vr)"/>
		<anim name="cbt_rifle_sup_walk_bwd" weight="cbt * sup * rifle * (1-vr)"/>
		<anim name="hos_pistol_sup_walk_bwd" weight="(hos + ntl) * sup * (1 - wnd) * pistol * (1-vr)"/>
		<anim name="cbt_pistol_sup_walk_bwd" weight="cbt * sup * pistol * (1-shield) * (1-vr)"/>

		<anim name="cbt_shield_crh_walk_bwd" weight="shield"/>

		<!-- VR --> 
		<anim name="ntl_rifle_idle_husk1_var1_l" weight="(1-akimbo_pistol) * husk1 * (cbt+hos) * vr" state_length_contrib="false"/>
		<anim name="cbt_rifle_crh_walk_bwd_vr" weight="rifle * husk1 * (cbt+hos) * vr"/>
		<anim name="cbt_pistol_crh_walk_bwd_vr" weight="pistol * husk1 * (cbt+hos) * vr"/>
		<anim name="cbt_bullpup_crh_walk_bwd_vr" weight="bullpup * husk1 * (cbt+hos) * vr"/>
		<!--<anim name="cbt_bow_crh_walk_bwd_vr" weight="bow * husk1 * (cbt+hos) * vr"/>-->
		<!-- End VR -->

		<to name="stand" redirect="std/stand/move/walk/bwd"/>
		<keys>
			<key at="enter" set_extension="anim_data" extension_key="move_bwd" extension_value="true"/>
			<key at="exit" set_extension="anim_data" extension_key="move_bwd" extension_value="false"/>
			<key at="enter" set_extension="anim_data" extension_key="move_side" extension_value="bwd"/>
			<key at="exit" set_extension="anim_data" extension_key="move_side" extension_value="false"/>
		</keys>
	</state>

	<state name="std/crouch/move/walk/l" type="mixloop" segment="base">
