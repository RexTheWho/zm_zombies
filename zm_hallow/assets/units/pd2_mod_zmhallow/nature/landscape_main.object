<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_mod_zmhallow/nature/landscape_main" orientation_object="rp_root" />
	
	<bodies>
		<body name="body_main" enabled="true" template="static">
			<object name="rp_root"/>
			<object name="g_land_grass"	collision_type="mesh_mopp" />
			<object name="g_rockwall"	collision_type="mesh_mopp" />
		</body>	
	</bodies>
	
	<decal_surfaces default_material="dirt" static="true">
	</decal_surfaces>
	
	<graphics>
		<graphic_group name="g_group" enabled="true" culling_object="co_culling">
			<object name="g_land_grass"	enabled="true" 	shadow_caster="true"/>
			<object name="g_moonglow"	enabled="true" 	shadow_caster="true"/>
			<object name="g_rockwall"	enabled="true" 	shadow_caster="true"/>
			<object name="g_water"		enabled="true" 	shadow_caster="true"/>
		</graphic_group>
	</graphics>
</dynamic_object>