			<!-- <object name="g_g"						enabled="true" 	shadow_caster="true"/> -->
			<object name="g_des_concrete"			enabled="true" 	shadow_caster="true"/>
			<object name="g_bry_concrete_floor_01"	enabled="true" 	shadow_caster="true"/>
			<object name="g_bry_rock1"				enabled="true" 	shadow_caster="true"/>
			<object name="g_lava"					enabled="true"/>
			<object name="g_mul"					enabled="true"/>
		</graphic_group>
	</graphics>
