<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_mod_zmhallow/architecture/underground/material_config" orientation_object="rp_root" />
	
	<bodies>
		<body name="body_main" enabled="true" template="static">
			<object name="rp_root"/>
			<!-- <object name="g_g"						collision_type="mesh_mopp" /> -->
			<object name="g_des_concrete"			collision_type="mesh_mopp" />
			<object name="g_bry_concrete_floor_01"	collision_type="mesh_mopp" />
			<object name="g_bry_rock1"				collision_type="mesh_mopp" />
		</body>
		<body name="body_mover_blocker" enabled="true" template="mover_blocker">
			<object name="c_mover_blocker"			collision_type="mesh_mopp" />
		</body>
		<body name="body_bag_blocker" enabled="true" template="bag" collision_class="bag_blocker">
