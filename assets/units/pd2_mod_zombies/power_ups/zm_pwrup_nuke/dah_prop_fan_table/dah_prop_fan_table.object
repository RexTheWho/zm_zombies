<dynamic_object>
	<diesel materials="units/pd2_dlc_dah/props/dah_prop_fan_table/dah_prop_fan_table" orientation_object="rp_dah_prop_fan_table" />
	<sequence_manager file="units/pd2_dlc_dah/props/dah_prop_fan_table/dah_prop_fan_table" />
	
	<animations>  
	    <animation_group name="a_top" enabled="true" speed="1" >
	      <object name="rp_dah_prop_fan_table" />
	      <object name="g_top_lod0" enabled="true" /> 
	      <object name="g_fan_top" enabled="true" />	             
