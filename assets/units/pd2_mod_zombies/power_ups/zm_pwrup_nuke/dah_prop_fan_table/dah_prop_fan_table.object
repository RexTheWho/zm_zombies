		<body name="body_collision_02" enabled="true" template="static">
			<object name="g_fan_top" />
			<object name="c_box_08" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_collision_03" enabled="true" template="static">
			<object name="g_fan_top" />
			<object name="c_box_07" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_collision_04" enabled="true" template="static">
			<object name="g_fan_top" />
			<object name="c_box_06" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_collision_05" enabled="true" template="static">
