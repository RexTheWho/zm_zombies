	      <object name="a_blades_lod0" enabled="true" />
	      <object name="a_blades_lod1" enabled="true" />
	      <object name="a_blades_lod2" enabled="true" />
	    </animation_group>

	    <animation_group name="a_papers" enabled="false" speed="1" >
	      <object name="a_fan_paper_01" enabled="true" />
	      <object name="a_fan_paper_02" enabled="true" />
	      <object name="a_fan_paper_03" enabled="true" />
	      <object name="g_paper01_g" enabled="true" /> 	
	      <object name="g_paper02_g" enabled="true" /> 
	      <object name="g_paper03_g" enabled="true" />	      
