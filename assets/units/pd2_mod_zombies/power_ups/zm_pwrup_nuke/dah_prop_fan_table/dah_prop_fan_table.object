			<object name="g_fan_top" />
			<object name="c_box_03" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_collision_08" enabled="true" template="static">
			<object name="g_fan_top" />
			<object name="c_box_02" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_collision_09" enabled="true" template="static">
			<object name="g_fan_top" />
			<object name="c_box_01" collision_type="box" padding="-2.5" />
		</body>
	</bodies>
	
	<decal_surfaces default_material="thin_layer">
		<decal_mesh name="dm_plastic" enabled="true" material="plastic" />
		<decal_mesh name="dm_plastic1" enabled="true" material="thin_layer" />
	</decal_surfaces>
	
	<graphics>
		<object name="g_fan_top" enabled="true" />
		<object name="g_paper01_g" enabled="false" />
		<object name="g_paper02_g" enabled="false" />
		<object name="g_paper03_g" enabled="false" />

		<lod_object name="lod_g_base">
			<object name="g_base_lod0" enabled="true" max_distance="1500" />
			<object name="g_base_lod1" enabled="true" max_distance="3000" />
			<object name="g_base_lod2" enabled="true" />
		</lod_object>

		<lod_object name="lod_g_blades">
			<object name="g_blades_lod0" enabled="true" max_distance="1500" />
			<object name="g_blades_lod1" enabled="true" max_distance="3000" />
